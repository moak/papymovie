{
  "name": "goldmovies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.16.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cz-emoji": "^1.3.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "next": "9.5.5",
    "next-auth": "^3.1.0",
    "next-i18next": "^6.0.3",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-rating-stars-component": "^2.2.0",
    "react-spring": "^8.0.27",
    "react-stars": "^2.2.5",
    "react-styled-flexboxgrid": "^3.2.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "undefined": "^0.1.0",
    "commitizen": "^4.0.3"
  },
  "peerOptionalDependencies": {
    "mongodb": "^3.6.0"
  },
  "config": {
    "cz-emoji": {
      "types": [
        {
          "name": "Feature",
          "emoji": "🌟",
          "description": "Adding new features",
          "code": "✅ [ADD]"
        },
        {
          "name": "Fix",
          "emoji": "🐞",
          "description": "Fixing issues or bugs",
          "code": "✴️ [FIX]"
        },
        {
          "name": "Refactoring",
          "emoji": "🎨",
          "description": "A code refactoring change",
          "code": "🔄 [MOD]"
        },
        {
          "name": "Chore",
          "emoji": "🔩",
          "description": "A chore change (modifying code, removing files/code, upgrading dependencies)",
          "code": "🔄 [MOD]"
        },
        {
          "name": "Documentation",
          "emoji": "📚",
          "description": "Documentation change (jsdoc, api, comments)",
          "code": "☑️ [DOC]"
        },
        {
          "name": "Tests",
          "emoji": "🔬",
          "description": "Adding missing tests or adjusting existing ones",
          "value": "🔀 [TEST]"
        }
      ],
      "scopes": [
        "core",
        "configuration",
        "components",
        "views",
        "design"
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "main": ".eslintrc.js",
  "author": "",
  "license": "ISC"
}
