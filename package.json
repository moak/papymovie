{
  "name": "papymovie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.0.3",
    "moment": "^2.29.3",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.0",
    "next": "12.1.6",
    "next-auth": "^4.5.0",
    "next-i18next": "^11.0.0",
    "react": "18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.2.0",
    "react-rating-stars-component": "^2.2.0",
    "react-spring": "^9.4.5",
    "react-stars": "^2.2.5",
    "react-styled-flexboxgrid": "^3.2.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.0.3",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1"
  },
  "peerOptionalDependencies": {
    "mongodb": "^3.6.0"
  },
  "config": {
    "cz-emoji": {
      "types": [
        {
          "name": "Feature",
          "emoji": "🌟",
          "description": "Adding new features",
          "code": "✅ [ADD]"
        },
        {
          "name": "Fix",
          "emoji": "🐞",
          "description": "Fixing issues or bugs",
          "code": "✴️ [FIX]"
        },
        {
          "name": "Refactoring",
          "emoji": "🎨",
          "description": "A code refactoring change",
          "code": "🔄 [MOD]"
        },
        {
          "name": "Chore",
          "emoji": "🔩",
          "description": "A chore change (modifying code, removing files/code, upgrading dependencies)",
          "code": "🔄 [MOD]"
        },
        {
          "name": "Documentation",
          "emoji": "📚",
          "description": "Documentation change (jsdoc, api, comments)",
          "code": "☑️ [DOC]"
        },
        {
          "name": "Tests",
          "emoji": "🔬",
          "description": "Adding missing tests or adjusting existing ones",
          "value": "🔀 [TEST]"
        }
      ],
      "scopes": [
        "core",
        "configuration",
        "components",
        "views",
        "design"
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "main": ".eslintrc.js",
  "author": "",
  "license": "ISC"
}
